<HorizontalOverflowNav>
  {{#each site.categories as |sc|}}
    {{#unless sc.parentCategory}}
      <li id="catNav-{{sc.slug}}">
        {{category-link
          sc
          extraClasses=(if (eq this.activeSlug sc.slug) "active")
        }}
      </li>
    {{/unless}}
  {{/each}}
</HorizontalOverflowNav>